@model Project1.WebApp.ViewModels.OrderViewModel

<table class="table">
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                Store
            </th>
            <th>
                Customer Name
            </th>
            <th>
                Price
            </th>
            <th>
                Time
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                @Html.DisplayFor(modelItem => Model.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.OrderedAt.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.OrderedBy.FirstName)
                @Html.DisplayFor(modelItem => Model.OrderedBy.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.TotalPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.OrderTime)
            </td>
        </tr>
    </tbody>
</table>